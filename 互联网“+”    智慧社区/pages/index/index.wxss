
/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  
  }
  
  .userinfo-avatar {
  width: 120rpx;
  height: 120rpx;
  margin: 60rpx;
  left: 10rpx;
  border-radius: 50%;
  }
  
  .userinfo-nickname {
  color: #aaa;
  }

  .box{
    display: flex;
    width: 100%;
    height: 100%;
    flex-wrap: wrap;
    justify-content: space-around;
    align-content: center;
    margin-top: -10rpx;
  
  }
  .box .item{
    display: flex;
    flex-direction: column;
    width: 320rpx;
    height: 320rpx;
    border-radius: 70rpx;
    background-color: rgba(192, 201, 201, 0.685);
    justify-content: center;
    align-items: center;
    margin-bottom: 40rpx;
  
  }
